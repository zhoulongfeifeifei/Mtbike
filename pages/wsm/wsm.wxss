.main{
   width: 100%;
   height: 100%;
   position: relative;
   font-family: PingFang SC; 
}
.ts{
  width: 92%;
  margin: 36rpx auto;
  font-size: 24rpx;
  color: #999;
}
.line{
  height: 24rpx;
  width: 4rpx;
  background: #fdd714;
  display: inline-block;
  margin-right: 20rpx;
}
.line1{
  height: 32rpx;
  width: 4rpx;
  background: #9a9a9a;
  display: inline-block;
  margin: 0 10rpx;
}
.userName,.Identification{
   width: 92%;
    height: 94rpx;
    margin: 20rpx auto;
    padding-left: 2%;
    background-color: #f3f3f3;
    border-radius: 10rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    font-size: 32rpx;
}
.nameLb,.IdentificationLb {
  flex: 1;
  font-weight:bold;
  font-family: Times New Roman;
}

.nameTf,.IdentificationTf{
  flex: 4;
   padding-left: 20rpx;
}
.information{
  width: 92%;
  margin: 0 auto;
  font-size: 24rpx;
  color: #999;
}
.loginBtn {
    width: 670rpx;
    height: 80rpx;
    line-height: 80rpx;
    margin: 40rpx auto;
    background: #262930;
    color: #fff;
    border-radius: 100rpx;
}